
CODE = intder95

F77 = f77
FFLAGS = -O

PSIPP = ./psipp/psipp

INCLUDES = ./

subdirs = \
psipp libparse intder_io

FSRC = \
ahx.F ahx2.F ahx3.F ahx4.F ahy2.F ahy3.F ahy4.F binvrt.F \
blkdta000.F brow.F disp.F dotx.F eq42.F expmat.F fcout.F fill3a.F \
fill3b.F fill4a.F fill4b.F formdk0.F formp.F gfmat.F gprgid.F h4th1.F \
h4th2.F h5th1.F hijks1.F hijks2.F hijks3.F hijks6.F hijks7.F hijks8.F \
hijks9.F hijs1.F hijs10.F hijs2.F hijs3.F hijs6.F hijs7.F hijs8.F hijs9.F \
hsry2.F hsry3.F hsry4.F inpfkm.F intder.F intder95.F \
lintr.F lprt.F machb.F machb2.F machf2.F machf3.F \
machf4.F machx.F machy.F machz.F massin.F mat1.F mat2.F normco.F nounfl.F \
numx.F numy.F numz.F openff.F orthog.F pder2.F pder3.F prjtst.F projk1.F \
projk2.F projk3.F projv.F rotc.F scapro.F sqmfc.F \
srtst1.F srtst2.F table1.F table2.F table3.F table4.F table5.F \
tripro.F vcder1.F vcxki.F \
vecpro.F vect1.F vect10.F vect2.F vect3.F vect4.F vect5.F vect6.F vect8.F \
vect9.F xf2.F xf3.F xin.F xout.F xrow.F yf2.F yin.F \
yin2.F yout.F yout2.F yrow.F zin.F zout.F

BINOBJ = $(FSRC:%.F=%.o)

all: default $(CODE)

default: 
	for dir in $(subdirs); \
           do \
             (cd $${dir}; echo ... Making in $${dir} ...; $(MAKE) default;) \
           done
	

$(CODE): $(BINOBJ) libparse/libparse.a intder_io/intder_io.a 
	$(F77) $(FFLAGS) $^ -o $(CODE)

%.o: %.F 

%.f: %.F
	$(PSIPP) -I$(INCLUDES) -o $@ $<

%.o: %.f ./psipp/stamp-psipp
	$(F77) $(FFLAGS) -c $<

./psipp/psipp:
	echo ... Making in ./psipp/psipp ...
	cd psipp; $(MAKE) default

libparse.a: ./psipp/psipp
	echo ... Making in ./libparse/libparse ...
	cd libparse; $(MAKE) default

intder_io.a: ./psipp/psipp
	echo ... Making in ./intder_io/intder_io ...
	cd intder_io; $(MAKE) default

clean: psipp_clean libparse_clean intder_io_clean
	/bin/rm -f $(CODE) *.o core

psipp_clean: 
	cd psipp; $(MAKE) clean

libparse_clean: 
	cd libparse; $(MAKE) clean

intder_io_clean:
	cd intder_io; $(MAKE) clean


